        <dependency>
            <groupId>uk.ac.leeds.ccg.andyt</groupId>
            <artifactId>agdt-java-generic-vector</artifactId>
            <version>${agdt-java-generic-vector.version}</version>
        </dependency>
    </dependencies>
    
    <repositories>
        <repository>
            <id>agdt-java-generic</id>
